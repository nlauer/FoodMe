<!-- Main hero unit for a primary marketing message or call to action -->
<div class="hero-unit">
    <h1>Register</h1>
    <p>Sign up for FoodMe, so we can order food for you with a click of a single button.</p>
</div>

<!-- Example row of columns -->
<form name="register" action="/register/complete/" method="POST">
  <fieldset>
    <legend>Basic Info</legend>
    <label>First Name</label>
    <input type="text" placeholder="Yagnik" name="first_name" />
    <label>Last Name</label>
    <input type="text" placeholder="Khanna" name="last_name" />
    <label>Email</label>
    <input type="text" placeholder="yagnik@isthebest.co" name="email" />
    <label>Password</label>
    <input type="password" placeholder="sriracha" name="password" />
    <label>Cell</label>
    <input type="text" placeholder="215-555-1029" name="phone_number" />
  </fieldset>

  <fieldset>
    <legend>Credit Card Info</legend>
      <label>Card Number</label>
      <input type="text" autocomplete="off" name="card-number" class="span4" />
      <label>CVC</label>
      <input type="text" autocomplete="off" name="cvc" class="span1"/>
      <label>Expiration (MM/YYYY)</label>
      <input type="text" class="span1" name="card-expiry-month"/>
      <span> / </span>
      <input type="text" class="span1" name="card-expiry-year"/>
  </fieldset>

    <div class="form-actions">
      <button type="submit" class="btn btn-primary">Register me!</button>
    </div>
</form>
